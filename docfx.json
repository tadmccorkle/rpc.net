{
  "metadata": [
    {
      "src": [
        {
          "files": ["**/*.csproj"],
          "exclude": ["**/bin/**", "**/obj/**"],
          "src": "src"
        }
      ],
      "dest": "obj/api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.yml"],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": ["toc.yml", "README.md"]
      }
    ],
    "resource": [],
    "externalReference": [],
    "markdownEngineName": "markdig",
    "globalMetadata": {
      "_appTitle": "TestLib",
      "_appFooter": "TestLib - Copyright Â© TM 2021<br>Generated by <strong>DocFX</strong>",
      "_enableSearch": true
    },
    "dest": "_site",
    "template": ["default", ".docfx/template"]
  }
}
